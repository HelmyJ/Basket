<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://java.sun.com/xml/ns/javaee" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_3_0.xsd" version="3.0">
  <servlet>
    <servlet-name>AffidcherCours</servlet-name>
    <servlet-class>controller.AfficherCours</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>Accueil</servlet-name>
    <jsp-file>/view/accueil.jsp</jsp-file>
  </servlet>
  <servlet-mapping>
    <servlet-name>AffidcherCours</servlet-name>
    <url-pattern>/coursDeSport</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>horaireCours</servlet-name>
    <url-pattern>/choixCours</url-pattern>
  </servlet-mapping>
</web-app>